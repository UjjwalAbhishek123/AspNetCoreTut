
@*
    It is our Partial View where we can write UI i.e. Presentation logic for the ViewComponent
*@

<div class="box">
    @* <h3>@((PersonGridModel)ViewData["NewGrid"]).GridTitle</h3> *@
    <h3>@ViewBag.NewGrid.GridTitle</h3>
    <table class="table w-100">
        <thead>
            <tr>
                <th>Sl. No</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach(Person person in ViewBag.NewGrid.Persons)
            {
                <tr>
                    <td>@person.PersonName</td>
                    <td>@person.jobTitle</td>
                </tr>
            }
        </tbody>
    </table>
</div>